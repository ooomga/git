body{
	margin:0px;
	padding:0px;
}
.header{
	height:68px;
	background-color: #EFEFEF;
	margin:0px;
	padding:0px;
}
.body{
	width:100%;
	border-color:blue;
	margin:0px;
	padding:0px;
}
.body>div{
	float:left;
}
.body:after{
	content:"";
	clear: both;
	display:block;
}
.menu{
	margin:0px;
	padding:0px;
	width:8.7%;
	height:inherit;
	border-color:red;
	background-color: lightgrey;
}
.content{
	margin:0px;
	padding:0px;
	width:90%;
}
.footer{
	margin:0px;
	padding:0px;
	width:100%;
	border-top: 1px solid grey;
}
.right_menu{
	height: 45px;
    border-bottom: 1px solid #E6E6E6;
    margin-left: 23px;
    line-height: 45px;
    font-size: 12px;
}
.grid-btn{
width:50px;
height:25px;
padding-top:4px;
border-radius: 3px;
display: inline-block;
background: #0E6EB8;
color: #fff;
font-size: 13px;
border: 0 solid #fff;
}