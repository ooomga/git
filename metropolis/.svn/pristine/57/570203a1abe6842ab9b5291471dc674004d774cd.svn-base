body{
	margin:0px;
	padding:0px;
}
.header{
	height:68px;
	background-color: grey;
	margin:0px;
	padding:0px;
}
.body{
	width:100%;
	border-color:blue;
	margin:0px;
	padding:0px;
}
.body>div{
	float:left;
}
.body:after{
	content:"";
	clear: both;
	display:block;
}
.menu{
	margin:0px;
	padding:0px;
	width:10%;
	border-color:red;
	background-color: lightgrey;
}
.content{
	margin:0px;
	padding:0px;
	width:90%;
}
.footer{
	margin:0px;
	padding:0px;
	border-top: 1px solid grey;
}